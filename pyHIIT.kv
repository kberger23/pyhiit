#:kivy 1.7.1
#:import Factory kivy.factory.Factory

<Screen>:
    canvas:
        Color:
            rgb: .2, .2, .2
        Rectangle:
            size: self.size

<Timer>:
	orientation: 'vertical'
    canvas.before:
        Color:
            rgba: .5, .5, .5, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height
    ClockLabel:
        #font_size: 30
        #canvas:
        #    Color:
        #        rgb: 1,0,1
        #    Line:
        #        circle:self.center_x, self.center_y, 90, 0, self.angle
        #        width: 5


<Buttons>:
    size: 1, 100
    size_hint: 1, None
    canvas.before:
        Color:
            rgba: .5, .5, .5, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height
	GridLayout:
		cols: 3
		size_hint: None, None
		size: 0, 0


<Overview>:
    orientation: "vertical"
	Timer:
        id: timer
	Buttons:
        id: buttons