#:kivy 1.7.1
#:import Factory kivy.factory.Factory

<Screen>:
    canvas:
        Color:
            rgb: .2, .2, .2
        Rectangle:
            size: self.size

<Timer>:
    canvas.before:
        Color:
            rgba: .5, .5, .5, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height
	BoxLayout:
		orientation: 'vertical'
		size: root.size
		pos: root.pos


<Buttons>:
    size: 1, 100
    size_hint: 1, None
    canvas.before:
        Color:
            rgba: .5, .5, .5, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height
	GridLayout:
		cols: 3
		size_hint: None, None
		size: 0, 0


<Overview>:
    orientation: "vertical"
	Timer:
	Buttons: